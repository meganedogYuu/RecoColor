{
  "extends": ["tslint-config-airbnb"],
  "rules": {
    "max-line-length": [true, 120],
    "ter-indent": [true, 2],
    "no-boolean-literal-compare": false,  //falseにしないといけない模様
    "strict-boolean-expressions": false,  //falseにしないといけない模様
    "trailing-comma": [ true, { "multiline": "never", "singleline": "never" }] //オブジェクト型の最後のカンマの設定
  }
}